; vim:ft=dosini

[module/xbacklight]
type = internal/xbacklight

format = <ramp> <bar>

ramp-0 = 
ramp-1 = 
ramp-2 = 
ramp-3 = 

bar-width = 10
bar-indicator = |
bar-indicator-font = 1
bar-indicator-foreground = ${root.foreground}

bar-fill = ─
bar-fill-font = 2
bar-fill-foreground = #9f78e1

bar-empty = ─
bar-empty-font = 2
bar-empty-foreground = ${colors.foreground-alt}
